<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.nayak.EMSystem.AdminMW"
            prefHeight="400.0" prefWidth="600.0">

    <top>
        <Label text="Education Management System" BorderPane.alignment="CENTER" />
    </top>
    <center>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="AddOrgnization" onMouseClicked="#handleMouseClick" mnemonicParsing="false" text="Add Organization" />
                <Button fx:id="AnalyzeData" onMouseClicked="#handleMouseClick" mnemonicParsing="false" text="Analyze Data" />
                </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
    </center>
</BorderPane>
