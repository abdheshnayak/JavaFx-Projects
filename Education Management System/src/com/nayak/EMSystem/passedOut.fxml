<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nayak.EMSystem.PassedOut">
   <top>
      <Label text="Pass A Student" BorderPane.alignment="CENTER">
         <padding>
            <Insets top="10.0" />
         </padding>
      </Label>
   </top>
   <center>
      <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Roll No." />
            <TextField fx:id="rollNo" promptText="Roll No." />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="closeBtn" onMouseClicked="#handleMouse" mnemonicParsing="false" text="Close" />
            <Button fx:id="submitBtn" onMouseClicked="#handleMouse" mnemonicParsing="false" text="Submit" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
