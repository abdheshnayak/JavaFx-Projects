<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.nayak.EMSystem.DropOut"
            prefHeight="400.0" prefWidth="600.0">

    <top>
        <Label text="Pass A Student" BorderPane.alignment="CENTER">
            <padding>
                <Insets top="10.0" />
            </padding>
        </Label>
    </top>
    <center>
        <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
            <children>
                <Label text="Roll No." />
                <TextField fx:id="rollNo" promptText="Roll No." />
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
    </center>
    <bottom>
        <HBox alignment="CENTER_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="closeBtn" onMouseClicked="#handleMouse" mnemonicParsing="false" text="Close" />
                <Button fx:id="submitBtn" onMouseClicked="#handleMouse" mnemonicParsing="false" text="Submit" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </bottom>
</BorderPane>
